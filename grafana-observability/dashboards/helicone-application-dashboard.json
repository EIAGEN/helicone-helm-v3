        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"default\", pod=~\"helicone-.*\", container!=\"POD\", container!=\"\"}[5m])) by (pod) * 100",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ],
