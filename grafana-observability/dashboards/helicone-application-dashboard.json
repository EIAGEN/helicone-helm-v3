          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Helicone Pod Restarts",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "__name__": true,
              "Time": true,
              "endpoint": true,
              "instance": true,
              "job": true,
              "namespace": true,
              "service": true,
              "uid": true
            },
            "indexByName": {},
            "renameByName": {
              "Value": "Restart Count",
              "container": "Container",
              "pod": "Pod"
            }
          }
        }
      ],
      "type": "table"
    },
